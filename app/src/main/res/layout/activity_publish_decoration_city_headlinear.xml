<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.sky.app.ui.activity.publish.PublishDecorationCityHeadlinearActivity">

    <include layout="@layout/publish_header" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.sky.app.library.component.banner.LoopViewPagerLayout
            android:id="@+id/city_mLoopViewPagerLayout1"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:background="@color/sky_color_white" />
        <ListView
            android:id="@+id/city_recycleview_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>

</LinearLayout>
